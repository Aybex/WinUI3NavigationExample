<Page
    x:Class="WinUI3NavigationExample.Views.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WinUI3NavigationExample.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    NavigationCacheMode="Required">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100px"></RowDefinition>
            <RowDefinition Height="100px"></RowDefinition>

        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Width="400px" Height="100px" Text="Home"/>
        <TextBox Grid.Row="1" PlaceholderText="Test" Text="Put text here" Width="400px" Height="100px"  ></TextBox>
    </Grid>
</Page>
